head
    meta(charset='utf-8')
    title #{title}
    base(href='#{base}')
    meta(http-equiv="X-UA-Compatible" content="IE=edge,chrome=1")
    meta(name="apple-mobile-web-app-capable" content="yes")
    meta(name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimal-ui")
    meta(name="apple-mobile-web-app-status-bar-style" content="yes")
    block links
        - var links=['mobile-angular-ui-hover.css','mobile-angular-ui-base.css','mobile-angular-ui-desktop.css']
        each link in links
            link(rel="stylesheet" href="/javascripts/dist/css/#{link}")
    block scripts
        -var scripts = ['angular.js','angular-route.js','mobile-angular-ui.js','mobile-angular-ui.gestures.js','jquery-1.10.2.min.js']
        each script in scripts
            script(src='/javascripts/dist/js/#{script}')





